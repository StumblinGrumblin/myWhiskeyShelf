<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <% @whiskies.each do |whisky| %>
    <div class="col-sm-12 col-xs-12 col-md-4 col-lg-3">
      <div class="thumbnail">
        <%= link_to whisky_path(whisky) do %>
          <div class="panel-body text-center">
            <%= image_tag(whisky.image_url) %>
            </div>
        <%- end %>
              <!-- <%= whisky.score %> -->
      </div>
    </div>
    <% end %>

    <%= link_to '+', new_path, class: "btn-new" %>
  </div>
</div>
